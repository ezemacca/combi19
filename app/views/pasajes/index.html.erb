<p id="notice"><%= notice %></p>

<h1>Pasajes</h1>

<table>
  <thead>
    <tr>
      <th>Precio</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pasajes.each do |pasaje| %>
      <tr>
        <td><%= pasaje.precio %></td>
        <td><%= pasaje. %></td>
        <td><%= link_to 'Show', pasaje %></td>
        <td><%= link_to 'Edit', edit_pasaje_path(pasaje) %></td>
        <td><%= link_to 'Destroy', pasaje, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pasaje', new_pasaje_path %>
